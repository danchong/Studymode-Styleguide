<section class="section-content">
  <div class="container">
 
    <div class="row">
      <div class="large-7 columns">
        {{> molecules-section-heading}}
      </div>
      
      <div class="large-5 columns">
        <div class="nav-tabs">
          {{# navTabItems}}
          {{> molecules-nav-tabs-item}}
          {{/ navTabItems}}
        </div>
      </div>
    </div>
       
    {{# exampleEssayCardsTopRated}}
    <div id="toprated" class="nav-tabs__content nav-tabs__content--active">
      <div class="row">
        {{# exampleEssayCards1}}
        <div class="large-4 columns">
          {{> molecules-card:card--label}}
        </div>
        {{/ exampleEssayCards1}}
      </div>
      
      <div class="row">
        {{# exampleEssayCards2}}
        <div class="large-4 columns">
          {{> molecules-card:card--label}}
        </div>
        {{/ exampleEssayCards2}}
      </div>
      
      <div class="row">
        <div class="large-4 columns">
          &nbsp;
        </div>
        <div class="large-4 columns">
          {{# exampleEssayCardsButton}}
          {{> atoms-button-link:button--inverse}}
          {{/ exampleEssayCardsButton}}
        </div>
        <div class="large-4 columns">
          &nbsp;
        </div>
      </div>
    </div>
    {{/ exampleEssayCardsTopRated}}
       
    {{# exampleEssayCardsNewest}}
    <div id="newest" class="nav-tabs__content">
      <div class="row">
        {{# exampleEssayCards1}}
        <div class="large-4 columns">
          {{> molecules-card:card--label}}
        </div>
        {{/ exampleEssayCards1}}
      </div>
      
      <div class="row">
        {{# exampleEssayCards2}}
        <div class="large-4 columns">
          {{> molecules-card:card--label}}
        </div>
        {{/ exampleEssayCards2}}
      </div>
      
      <div class="row">
        <div class="large-4 columns">
          &nbsp;
        </div>
        <div class="large-4 columns">
          {{# exampleEssayCardsButton}}
          {{> atoms-button-link:button--inverse}}
          {{/ exampleEssayCardsButton}}
        </div>
        <div class="large-4 columns">
          &nbsp;
        </div>
      </div>
    </div>
    {{/ exampleEssayCardsNewest}}
    
    
    
  </div>
</section>
