        &lt;div class=&quot;large-4 columns&quot;&gt;
          {{&gt; molecules-card:card--label}}
        &lt;/div&gt;
        {{/ exampleEssayCards1}}
      &lt;/div&gt;
      
      &lt;div class=&quot;row&quot;&gt;
        {{# exampleEssayCards2}}
