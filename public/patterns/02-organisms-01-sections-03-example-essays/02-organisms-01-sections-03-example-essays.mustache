        {{/ exampleEssayCards2}}
      &lt;/div&gt;
      
      &lt;div class=&quot;row&quot;&gt;
        &lt;div class=&quot;large-4 columns&quot;&gt;
          &amp;nbsp;
        &lt;/div&gt;
        &lt;div class=&quot;large-4 columns&quot;&gt;
          {{# exampleEssayCardsButton}}
          {{&gt; atoms-button-link:button--inverse}}
          {{/ exampleEssayCardsButton}}
        &lt;/div&gt;
        &lt;div class=&quot;large-4 columns&quot;&gt;
          &amp;nbsp;
        &lt;/div&gt;
      &lt;/div&gt;
    &lt;/div&gt;
    {{/ exampleEssayCardsTopRated}}
       
    {{# exampleEssayCardsNewest}}
    &lt;div id=&quot;newest&quot; class=&quot;nav-tabs__content&quot;&gt;
