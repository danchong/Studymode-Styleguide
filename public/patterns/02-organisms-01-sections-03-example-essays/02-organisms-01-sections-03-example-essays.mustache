&lt;section class=&quot;section-content&quot;&gt;
  &lt;div class=&quot;container&quot;&gt;
 
    &lt;div class=&quot;row&quot;&gt;
      &lt;div class=&quot;large-7 columns&quot;&gt;
        {{&gt; molecules-section-heading}}
      &lt;/div&gt;
      
      &lt;div class=&quot;large-5 columns&quot;&gt;
        &lt;div class=&quot;nav-tabs&quot;&gt;
          {{# navTabItems}}
          {{&gt; molecules-nav-tabs-item}}
          {{/ navTabItems}}
        &lt;/div&gt;
      &lt;/div&gt;
    &lt;/div&gt;
       
    {{# exampleEssayCardsTopRated}}
